{
  "name": "rdf-serve",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test": "jest",
    "lint": "eslint lib/* __tests__/* --ext .ts",
    "lint:fix": "eslint lib/* __tests__/* --ext .ts --fix",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeswr/rdf-serve.js"
  },
  "keywords": [
    "RDF",
    "serve",
    "server",
    "content",
    "negotiation",
    "type",
    "turtle",
    "jsonld"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/rdf-serve.js/issues"
  },
  "homepage": "https://github.com/jeswr/rdf-serve.js#readme",
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.3.1",
    "pre-commit": "^1.2.2",
    "semantic-release": "^20.0.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "express": "^4.18.2",
    "get-port-please": "^3.0.1",
    "negotiator": "^0.6.3",
    "rdf-transform": "^1.0.0"
  },
  "peerDependencies": {
    "@types/express": "^4.17.16",
    "@types/negotiator": "^0.6.1"
  },
  "bin": "./dist/bin/index.js"
}
